{
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[latex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop"
    },
    "[markdown]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.quickSuggestions": {
            "comments": "on",
            "other": "on",
            "strings": "on"
        },
        "editor.quickSuggestionsDelay": 1000000,
        "editor.suggest.showSnippets": true,
        "editor.suggestOnTriggerCharacters": true,
        "editor.wordBasedSuggestions": false
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        // Enable parameter hints
        "editor.parameterHints.enabled": true,
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "on",
            "strings": "off"
        },
        "editor.suggest.localityBonus": true,
        "editor.wordWrapColumn": 80
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "black-formatter.args": ["--line-length", "80"],
    "debug.console.acceptSuggestionOnEnter": "on",
    "debug.console.fontFamily": "JetBrainsMono Nerd Font",
    "debug.console.lineHeight": 15,
    "debug.terminal.clearBeforeReusing": true,
    "diffEditor.codeLens": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.bracketPairColorization.enabled": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorSurroundingLines": 20,
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "JetBrainsMono Nerd Font, Times New Roman",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.lineHeight": 1.6,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.guides.bracketPairs": "active",
    "editor.guides.bracketPairsHorizontal": true,
    "editor.inlineSuggest.enabled": true,
    "editor.language.colorizedBracketPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\\[", "\\]"],
        ["\\(", "\\)"]
    ],
    "editor.largeFileOptimizations": false,
    "editor.minimap.enabled": false,
    "editor.padding.bottom": 40,
    "editor.padding.top": 20,
    "editor.renderLineHighlight": "all",
    "editor.renderWhitespace": "boundary",
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.stickyTabStops": true,
    "editor.wordWrap": "bounded",
    "explorer.autoReveal": true,
    "explorer.confirmDelete": false,
    "extensions.experimental.affinity": {
        "ms-python.python": 1,
        "ms-python.vscode-pylance": 1,
        "ms-toolsai.jupyter": 1,
        "ms-toolsai.jupyter-renderers": 1
    },
    "files.associations": {
        "*.html": "html",
        "*.njk": "html",
        "*.rmd": "markdown",
        "*.tmpl": "html"
    },
    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "github.copilot-labs.advanced": {
        "length": 500
    },
    "github.copilot.advanced": {
        "inlineSuggestCount": 5,
        "length": 500,
        "temperature": "0.4"
    },
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": true,
        "python": true,
        "rust": false,
        "go": false
    },
    "html.format.templating": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "jupyter.askForKernelRestart": false,
    "jupyter.generateSVGPlots": true,
    "jupyter.interactiveWindowMode": "perFile",
    "jupyter.sendSelectionToInteractiveWindow": true,
    "jupyter.useDefaultConfigForJupyter": false,
    "jupyter.widgetScriptSources": ["jsdelivr.com", "unpkg.com"],
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.intellisense.unimathsymbols.enabled": true,
    "latex-workshop.latex.magic.args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
    ],
    "latex-workshop.latex.recipe.default": "latexmk (pdflatex)",
    "latex-workshop.latex.recipes": [
        {
            "name": "latexmk (pdflatex)",
            "tools": ["pdflatex"]
        },
        {
            "name": "latexmk (xelatex)",
            "tools": ["xelatexmk"]
        }
    ],
    "latex-workshop.latex.tools": [
        {
            "args": [
                "--shell-escape", // if you want to have the shell-escape flag
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "command": "pdflatex",
            "name": "pdflatex"
        },
        {
            "args": [
                "--shell-escape",
                "-xelatex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "command": "latexmk",
            "name": "xelatexmk"
        }
    ],
    "latex-workshop.synctex.afterBuild.enabled": true,
    "latex-workshop.view.autoFocus.enabled": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "markdown-preview-enhanced.breakOnSingleNewLine": true,
    "markdown-preview-enhanced.enableExtendedTableSyntax": true,
    "markdown-preview-enhanced.enableTypographer": true,
    "markdown-preview-enhanced.frontMatterRenderingOption": "code block",
    "markdown-preview-enhanced.imageFolderPath": "/images",
    "markdown-preview-enhanced.latexEngine": "pdflatex",
    "markdown-preview-enhanced.liveUpdate": true,
    "markdown-preview-enhanced.mathBlockDelimiters": [
        ["$$", "$$"],
        ["\\[", "\\]"]
    ],
    "markdown-preview-enhanced.mathInlineDelimiters": [
        ["$", "$"],
        ["\\(", "\\)"]
    ],
    "markdown-preview-enhanced.previewTheme": "one-light.css",
    "markdown.extension.tableFormatter.normalizeIndentation": true,
    "markdown.preview.lineHeight": 1.2,
    "markdown.trace": "verbose",
    "markdownlint.focusMode": true,
    "material-icon-theme.activeIconPack": "react_redux",
    "notebook.cellFocusIndicator": "border",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.compactView": false,
    "notebook.consolidatedRunButton": true,
    "notebook.displayOrder": ["text/html", "image/png"],
    "notebook.output.textLineLimit": 80,
    "python.defaultInterpreterPath": "/Users/seba/.pyenv/versions/3.10.3/envs/adm-booth/bin/python",
    "notebook.showFoldingControls": "always",
    "pasteImage.filePathConfirmInputBoxMode": "onlyName",
    "pasteImage.path": "${currentFileDir}/assets/images",
    "pasteImage.prefix": "",
    "pasteImage.showFilePathConfirmInputBox": true,
    "peacock.affectSashHover": false,
    "peacock.affectStatusBar": false,
    "peacock.favoriteColors": [
        {
            "name": "Angular Red",
            "value": "#dd0531"
        },
        {
            "name": "Azure Blue",
            "value": "#007fff"
        },
        {
            "name": "JavaScript Yellow",
            "value": "#f9e64f"
        },
        {
            "name": "Mandalorian Blue",
            "value": "#1857a4"
        },
        {
            "name": "Node Green",
            "value": "#215732"
        },
        {
            "name": "React Blue",
            "value": "#61dafb"
        },
        {
            "name": "Something Different",
            "value": "#832561"
        },
        {
            "name": "Svelte Orange",
            "value": "#ff3d00"
        },
        {
            "name": "Vue Green",
            "value": "#42b883"
        }
    ],
    "prettier.proseWrap": "never",
    "prettier.tabWidth": 4,
    "projectManager.git.baseFolders": ["/Users/seba/projects"],
    "projectManager.ignoreProjectsWithinProjects": true,
    "python.analysis.autoImportCompletions": false,
    "python.analysis.completeFunctionParens": true,
    "python.formatting.provider": "black",
    "python.linting.flake8Enabled": true,
    "python.linting.mypyCategorySeverity.note": "Warning",
    "python.linting.pydocstyleEnabled": false,
    "python.linting.pylamaArgs": ["--max-line-length=80"],
    "python.linting.pylintArgs": [
        "--load-plugins",
        "perflint",
        "--rcfile",
        "${workspaceFolder}/.pylintrc"
    ],
    "python.sortImports.args": ["--profile", "black"],
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.terminal.activateEnvironment": true,
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "sqltools.connections": [],
    "tabnine.experimentalAutoImports": false,
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1",
        "PYTHONPATH": "${workspaceFolder}/__pypackages__/3.10/lib"
    },
    "terminal.integrated.inheritEnv": false,
    "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "after": ["<Esc>"],
            "before": ["j", "j"]
        },
        {
            "after": ["<Esc>"],
            "before": ["up"]
        },
        {
            "after": ["<Esc>"],
            "before": ["down"]
        },
        {
            "after": ["<Esc>"],
            "before": ["right"]
        },
        {
            "after": ["<Esc>"],
            "before": ["left"]
        }
    ],
    "vim.leader": "<space>",
    "vim.normalModeKeyBindings": [
        {
            "before": ["<leader>", "m"],
            "commands": ["bookmarks.toggle"]
        },
        {
            "before": ["<leader>", "b"],
            "commands": ["bookmarks.list"]
        },
        {
            "after": ["<Esc>"],
            "before": ["up"]
        },
        {
            "after": ["<Esc>"],
            "before": ["down"]
        },
        {
            "after": ["<Esc>"],
            "before": ["right"]
        },
        {
            "after": ["<Esc>"],
            "before": ["left"]
        }
    ],
    "vim.overrideCopy": false,
    "vim.smartRelativeLine": true,
    "vim.statusBarColorControl": false,
    "vim.useSystemClipboard": true,
    "vim.visualModeKeyBindings": [
        {
            "before": [">"],
            "commands": ["editor.action.indentLines"]
        },
        {
            "before": ["<"],
            "commands": ["editor.action.outdentLines"]
        },
        {
            "after": ["<Esc>"],
            "before": ["up"]
        },
        {
            "after": ["<Esc>"],
            "before": ["down"]
        },
        {
            "after": ["<Esc>"],
            "before": ["right"]
        },
        {
            "after": ["<Esc>"],
            "before": ["left"]
        }
    ],
    "window.openFilesInNewWindow": "on",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    // "workbench.fontAliasing": "auto",
    "workbench.iconTheme": "Monokai Pro Icons",
    "workbench.tree.indent": 15,
    "workspace.searchLimit": 100,
    "editor.accessibilitySupport": "off",
    "files.exclude": {
        "**/.trunk/*out": true
    },
    "files.watcherExclude": {
        "**/.trunk/*out": true
    },
    "trunk.trunkGrayOutNonBlockingIssues": false,
    "workbench.colorCustomizations": {
        "editorInlayHint.background": "#1e1e1e00",
        "editorInlayHint.foreground": "#95a8ff"
    },
    "python.pylanceLspNotebooksEnabled": true,
    "jupyter.pylanceHandlesNotebooks": true,
    "editor.inlayHints.fontSize": 12,
    "mplstyle.hover.matplotlibPath": "/home/sbarrios/miniconda3/envs/path-analysis/lib/python3.10/site-packages/matplotlib",
    "hdinsight.disableOpenSurveyLink": true,
    "jupyter.diagnostics.reservedPythonNames.exclude": [
        "**/site-packages/**",
        "**/lib/python/**",
        "**/lib64/python/**",
        "/Users/sbarrios/code/personal/amazon-ynab/amazon_ynab/__main__.py"
    ],
    "mypy.dmypyExecutable": "~/.pyenv/versions/base/bin/dmypy",
    "editor.inlayHints.enabled": true,
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "workbench.colorTheme": "Monokai Pro",
    "workbench.editor.splitInGroupLayout": "vertical",
    "window.commandCenter": true,
    "window.dialogStyle": "custom",
    "python.linting.mypyCategorySeverity.error": "Warning",
    "[properties]": {
        "editor.defaultFormatter": "trunk.io"
    },
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    }
}
