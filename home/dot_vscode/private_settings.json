{
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[django-html]": {
        "editor.defaultFormatter": "monosans.djlint"
    },
    "[dockerfile]": {
        "editor.defaultFormatter": "ms-azuretools.vscode-docker"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[ignore]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint",
        "editor.quickSuggestions": {
            "comments": "on",
            "other": "on",
            "strings": "on"
        },
        "editor.quickSuggestionsDelay": 1000000,
        "editor.suggest.showSnippets": true,
        "editor.suggestOnTriggerCharacters": true,
        "editor.tabSize": 2,
        "editor.wordBasedSuggestions": false
    },
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        },
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        // Enable parameter hints
        "editor.parameterHints.enabled": true,
        "editor.quickSuggestions": {
            "comments": "off",
            "other": "on",
            "strings": "off"
        },
        "editor.suggest.localityBonus": true,
        "editor.wordWrapColumn": 100
    },
    "[r]": {
        "editor.defaultFormatter": "REditorSupport.r"
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust-analyzer",
        "editor.parameterHints.enabled": true,
        "editor.quickSuggestions": {
            "comments": "on",
            "other": "on",
            "strings": "off"
        },
        "editor.suggest.localityBonus": true,
        "editor.suggest.showWords": false
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[sql]": {
        "editor.defaultFormatter": "adpyke.vscode-sql-formatter"
    },
    "[tex]": {
        "editor.defaultFormatter": "James-Yu.latex-workshop",
        "editor.wordWrapColumn": 120
    },
    "[toml]": {
        "editor.defaultFormatter": "tamasfe.even-better-toml"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.tabSize": 2
    },
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "catppuccin.boldKeywords": false,
    "configurationSync.store": {
        "authenticationProviders": {
            "gitpod": {
                "scopes": [
                    "function:accessCodeSyncStorage",
                    "function:getLoggedInUser",
                    "resource:default"
                ]
            }
        },
        "canSwitch": false,
        "insidersUrl": "https://gitpod.io/code-sync",
        "stableUrl": "https://gitpod.io/code-sync",
        "url": "https://gitpod.io/code-sync"
    },
    "css.lint.unknownAtRules": "ignore",
    "debug.console.fontFamily": "JetBrainsMono Nerd Font",
    "debug.console.lineHeight": 15,
    "debug.terminal.clearBeforeReusing": true,
    "diffEditor.codeLens": true,
    "diffEditor.wordWrap": "off",
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorSurroundingLines": 20,
    "editor.find.autoFindInSelection": "multiline",
    "editor.fontFamily": "JetBrainsMonoNF-Medium, Menlo",
    "editor.fontLigatures": true,
    "editor.fontSize": 14,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file",
    "editor.guides.bracketPairs": true,
    "editor.guides.indentation": false,
    "editor.inlayHints.enabled": "offUnlessPressed",
    "editor.inlayHints.fontSize": 10,
    "editor.inlayHints.padding": true,
    "editor.inlineSuggest.enabled": true,
    "editor.largeFileOptimizations": false,
    "editor.lineHeight": 1.4,
    "editor.minimap.enabled": false,
    "editor.padding.bottom": 40,
    "editor.padding.top": 20,
    "editor.renderLineHighlight": "none",
    "editor.renderLineHighlightOnlyWhenFocus": true,
    "editor.renderWhitespace": "none",
    "editor.scrollbar.verticalScrollbarSize": 8,
    "editor.showFoldingControls": "always",
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": true,
    "editor.stickyTabStops": true,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 120,
    "emmet.includeLanguages": {
        "django-html": "html",
        "erb": "html"
    },
    "errorLens.enabled": true,
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info",
        "hint"
    ],
    "errorLens.excludeBySource": [
        "jsonc(520)"
    ],
    "errorLens.lintFilePaths": {
        "Stylelint": [
            "**/*.stylelintrc",
            "**/*.stylelintrc.{cjs,js,json,yaml,yml}",
            "**/*stylelint.config.{cjs,js}",
            "**/*package.json"
        ],
        "eslint": [
            "**/*.eslintrc.{js,cjs,yaml,yml,json}",
            "**/*package.json"
        ]
    },
    "explorer.autoReveal": true,
    "explorer.confirmDelete": false,
    "extensions.experimental.affinity": {
        "ms-python.python": 2,
        "ms-python.vscode-pylance": 2,
        "ms-toolsai.jupyter": 2,
        "ms-toolsai.jupyter-renderers": 2
    },
    "files.associations": {
        "**/*.html": "html",
        "**/requirements{/**,*}.{txt,in}": "pip-requirements",
        "**/templates/**/**.html": "django-html",
        "*.njk": "html",
        "*.rmd": "markdown",
        "*.tmpl": "html"
    },
    "files.autoSave": "onFocusChange",
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.trunk/*out": true
    },
    "git.autoRepositoryDetection": "subFolders",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.openRepositoryInParentFolders": "never",
    "github.copilot-labs.advanced": {
        "length": 500
    },
    "github.copilot.advanced": {
        "inlineSuggestCount": 5,
        "length": 500,
        "temperature": "0.4"
    },
    "github.copilot.enable": {
        "*": true,
        "css": false,
        "dart": true,
        "go": false,
        "html": false,
        "javascript": true,
        "javascriptreact": false,
        "markdown": true,
        "plaintext": true,
        "python": true,
        "ruby": false,
        "rust": false,
        "scminput": false,
        "typescript": false,
        "yaml": true
    },
    "githubIssues.issueBranchTitle": "issue${issueNumber}/${sanitizedLowercaseIssueTitle}",
    "githubPullRequests.fileListLayout": "flat",
    "go.toolsManagement.autoUpdate": true,
    "html.format.templating": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindow.creationMode": "perFile",
    "jupyter.interactiveWindow.textEditor.executeSelection": true,
    "jupyter.useDefaultConfigForJupyter": false,
    "jupyter.widgetScriptSources": [
        "jsdelivr.com",
        "unpkg.com"
    ],
    "liveServer.settings.donotShowInfoMsg": true,
    "lldb.library": "/Applications/Xcode.app/Contents/SharedFrameworks/LLDB.framework/Versions/A/LLDB",
    "notebook.cellFocusIndicator": "border",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "notebook.compactView": false,
    "notebook.consolidatedRunButton": true,
    "notebook.displayOrder": [
        "text/html",
        "image/png"
    ],
    "notebook.formatOnSave.enabled": true,
    "notebook.output.textLineLimit": 80,
    "notebook.showFoldingControls": "always",
    "pasteImage.filePathConfirmInputBoxMode": "onlyName",
    "pasteImage.path": "${currentFileDir}/assets/images",
    "pasteImage.prefix": "",
    "pasteImage.showFilePathConfirmInputBox": true,
    "prettier.proseWrap": "never",
    "prettier.requireConfig": true,
    "prettier.tabWidth": 4,
    "projectManager.git.baseFolders": [
        "/Users/seba/projects"
    ],
    "projectManager.ignoreProjectsWithinProjects": true,
    "python.analysis.autoImportCompletions": false,
    "python.analysis.completeFunctionParens": true,
    "python.formatting.provider": "black",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.terminal.activateEnvironment": true,
    "redhat.telemetry.enabled": false,
    "remote.SSH.defaultExtensions": [
        "gitpod.gitpod-remote-ssh"
    ],
    "rust-analyzer.check.command": "clippy",
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.copyOnSelection": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "terminal.integrated.inheritEnv": false,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "REVIEW",
        "[ ]",
        "[x]"
    ],
    "todo-tree.highlights.backgroundColourScheme": [
        "red",
        "yellow",
        "green",
        "blue",
        "indigo",
        "violet"
    ],
    "vim.easymotion": true,
    "vim.foldfix": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "j",
                "j"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "up"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "down"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "right"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "left"
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.normalModeKeyBindingsNonRecursive": [
        // NAVIGATION
        // switch b/w buffers
        {
            "before": [
                "<S-h>"
            ],
            "commands": [
                ":bprevious"
            ]
        },
        {
            "before": [
                "<S-l>"
            ],
            "commands": [
                ":bnext"
            ]
        },
        // splits
        {
            "before": [
                "leader",
                "v"
            ],
            "commands": [
                ":vsplit"
            ]
        },
        {
            "before": [
                "leader",
                "s"
            ],
            "commands": [
                ":split"
            ]
        },
        // panes
        {
            "before": [
                "leader",
                "h"
            ],
            "commands": [
                "workbench.action.focusLeftGroup"
            ]
        },
        {
            "before": [
                "leader",
                "j"
            ],
            "commands": [
                "workbench.action.focusBelowGroup"
            ]
        },
        {
            "before": [
                "leader",
                "k"
            ],
            "commands": [
                "workbench.action.focusAboveGroup"
            ]
        },
        {
            "before": [
                "leader",
                "l"
            ],
            "commands": [
                "workbench.action.focusRightGroup"
            ]
        },
        // FILE TREE
        {
            "before": [
                "leader",
                "n",
                "e"
            ],
            "commands": [
                "workbench.explorer.fileView.focus"
            ]
        },
        // NICE TO HAVE
        {
            "before": [
                "leader",
                "w"
            ],
            "commands": [
                "workbench.action.files.save"
            ]
        },
        {
            "before": [
                "leader",
                "f"
            ],
            "commands": [
                "workbench.action.quickOpen"
            ]
        },
        {
            "before": [
                "leader",
                "p"
            ],
            "commands": [
                "editor.action.formatDocument"
            ]
        },
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "g",
                "d"
            ],
            "commands": [
                "editor.action.goToDeclaration"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<C-o>"
            ],
            "commands": [
                "workbench.action.navigateBack"
            ]
        },
        {
            "before": [
                "<C-i>"
            ],
            "commands": [
                "workbench.action.navigateForward"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "up"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "down"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "right"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "left"
            ]
        }
    ],
    "vim.overrideCopy": false,
    "vim.smartRelativeLine": true,
    "vim.statusBarColorControl": false,
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "leader",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "up"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "down"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "right"
            ]
        },
        {
            "after": [
                "<Esc>"
            ],
            "before": [
                "left"
            ]
        }
    ],
    "vsicons.dontShowNewVersionMessage": true,
    "window.commandCenter": true,
    "window.dialogStyle": "custom",
    "window.openFilesInNewWindow": "on",
    "window.titleBarStyle": "native",
    "workbench.activityBar.visible": false,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "workbench.colorTheme": "Catppuccin Mocha Bordered",
    "workbench.editor.splitInGroupLayout": "vertical",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.pdf": "latex-workshop-pdf-hook"
    },
    "workbench.fontAliasing": "auto",
    "workbench.iconTheme": "vscode-icons",
    "workbench.tree.indent": 10,
    "workspace.searchLimit": 1000,
    "git.ignoredRepositories": [
        "/Users/seba/.vscode/extensions/stepsize.stepsize-1.0.15/resources/demo-repository"
    ],
    "vim.history": 200
}